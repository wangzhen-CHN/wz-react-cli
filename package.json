    "core-js": "^3.17.2",
    "dayjs": "^1.10.6",
    "lodash-es": "^4.17.21",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "recoil": "^0.4.1"
  }
