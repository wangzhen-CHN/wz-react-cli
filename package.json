    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
